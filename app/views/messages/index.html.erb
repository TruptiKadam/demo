<p style="color: green"><%= notice %></p>

<h1>Messages</h1>
<%= link_to "New message", new_message_path %>
<br></br>

<div id="messages">
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Message</th>
    </tr>
    <% @messages.each do |message| %>
      <%= render message %>
    <% end %>
  </table>
</div>

